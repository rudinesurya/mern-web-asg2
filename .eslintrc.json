{
  "extends": [
    "airbnb"
  ],
  "rules": {
    "no-console": "off",
    "no-unused-vars": "warn",
    "no-param-reassign": "off",
    "no-plusplus": "off",
    "func-names": "off",
    "no-shadow": "off",
    "no-use-before-define": "off",
    "no-restricted-globals": "off",
    "no-restricted-syntax": "off",
    "consistent-return": "off",
    "no-underscore-dangle": [
      "error",
      {
        "allow": [
          "_id"
        ]
      }
    ]
  }
}
